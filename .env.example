# Discord Bot Configuration
# 从 Discord Developer Portal 获取的机器人Token
BOT_TOKEN=your_discord_bot_token_here

# AliExpress Monitor Configuration
# AliExpress监控频道的Discord频道ID
ALIEXPRESS_CHANNEL_ID=your_aliexpress_channel_id
# 要监控的AliExpress商品URL
ALIEXPRESS_PRODUCT_URL=https://www.aliexpress.com/item/1005007198494636.html
# 检查间隔（秒）
ALIEXPRESS_MIN_INTERVAL=3
ALIEXPRESS_MAX_INTERVAL=8
# 心跳通知间隔（秒）
ALIEXPRESS_HEARTBEAT_INTERVAL=180
# 库存通知间隔（秒）
ALIEXPRESS_NOTIFICATION_INTERVAL=3

# PopMart Official Website Monitor Configuration  
# PopMart官网监控频道的Discord频道ID
OFFICIAL_CHANNEL_ID=your_official_channel_id
# 要监控的PopMart官网商品URL
OFFICIAL_PRODUCT_URL=https://www.popmart.com/your-product-url
# 检查间隔（秒）
OFFICIAL_MIN_INTERVAL=2
OFFICIAL_MAX_INTERVAL=5
# 心跳通知间隔（秒）
OFFICIAL_HEARTBEAT_INTERVAL=120
# 库存通知间隔（秒）
